class Person{
    constructor(name,age,rollno,standard,height,weight,mothertounge,bloodgrp,nationality){
        this.name = name;
        this.age = age;
        this.rollno = rollno;
        this.standard = standard;
        this.height = height;
        this.weight = weight;
        this.mothertounge = mothertounge;
        this.bloodgrp = bloodgrp;
        this.nationality = nationality;
    }
    details(){
      console.log(`
      name:${this.name}
      age:${this.age}
      rollno:${this.rollno}
      standard:${this.standard}
      height:${this.height}
      weight:${this.weight} 
      mothertounge:${this.mothertounge}
      bloodgrp:${this.bloodgrp}
      nationality:${this.nationality}
      `)
    }
}
   let Object = new Person("grace","22","2268299","12th","5.6","tamil","o+ve","indian")
   Object.details()